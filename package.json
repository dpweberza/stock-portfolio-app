{
  "name": "stock-portfolio-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0"
  },
  "proxy": "http://localhost:8000",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "concurrently \"react-scripts-ts build\" \"tsc -p server\"",
    "dev": "concurrently \"react-scripts-ts start\" \"tsc -w -p server\"  \"nodemon dist/server.js\"",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "concurrently": "^3.5.1",
    "typescript": "^2.9.2"
  }
}
