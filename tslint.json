{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "server/**/*.ts",
      "dist/**/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "triple-equals": false
  }
}
